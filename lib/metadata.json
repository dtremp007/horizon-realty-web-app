{
  "listings": {
    "fields": {
      "address": {
        "dataType": "string",
        "options": "None"
      },
      "bathrooms": {
        "dataType": "number",
        "options": "None"
      },
      "bedrooms": {
        "dataType": "number",
        "options": "None"
      },
      "coordinates": {
        "dataType": "array",
        "arrayDataType": "number",
        "options": "None",
        "filterIncompatible": true
      },
      "currency": {
        "dataType": "string",
        "options": ["USD", "MXN"],
        "filterIncompatible": true
      },
      "description": {
        "dataType": "string",
        "options": "None",
        "filterIncompatible": true
      },
      "electricity": {
        "dataType": "boolean",
        "options": "None"
      },
      "houseSize": {
        "dataType": "number",
        "options": "None",
        "unit": "houseSizeUnits"
      },
      "houseSizeUnits": {
        "dataType": "string",
        "options": ["SQ.FT."]
      },
      "imageUrls": {
        "dataType": "array",
        "arrayDataType": "string",
        "options": "None",
        "filterIncompatible": true
      },
      "landArea": {
        "dataType": "number",
        "options": "None",
        "unit": "landAreaUnits"
      },
      "landAreaUnits": {
        "dataType": "number",
        "options": ["ACRES", "HECTARES"],
        "filterIncompatible": true
      },
      "listingType": {
        "dataType": "array",
        "options": ["LOTE", "CASA", "BODEGA", "LOTES_COMERCIALES", "LOTES_RESIDENCIALES", "LABORES/RANCHOS", "APARTMENTS"],
        "arrayDataType": "string"
      },
      "paymentType": {
        "dataType": "string",
        "options": "None"
      },
      "price": {
        "dataType": "number",
        "options": "None",
        "unit": "currency"
      },
      "title": {
        "dataType": "string",
        "options": "None",
        "filterIncompatible": true
      },
      "water": {
        "dataType": "boolean",
        "options": "None"
      }
    }
  },

  "filters": {
    "defaultOptions": {
      "id": {
        "type": "text",
        "label": "Filter Name"
      },
      "legend": {
        "type": "text",
        "label": "Legend"
      },
      "fieldKey": {
        "type": "select",
        "data": {
          "functionCall": "getCompatibleFieldKeys",
          "arguments": []
        },
        "label": "Compare Against..."
      },
      "fallback": {
        "type": "text",
        "label": "Fallback"
      }
    },
    "defaultParameters": {
      "id": "Any",
      "fieldKey": "Any",
      "active": false,
      "comparisonOperator": "==="
    },
    "ofType": {
      "NativeSelect": {
        "useWith": {
          "type": "array",
          "exclude": "",
          "only": ""
        },
        "defaultParameters": {
          "type": "NativeSelect",
          "filterValue": "Option1",
          "fallback": "Option3",
          "filterProps": {
            "data": [
              { "label": "Option1", "value": "Option1" },
              { "label": "Option2", "value": "Option2" },
              { "label": "Option3", "value": "Option3" }
            ]
          }
        },
        "options": {
          "fallback": {
            "type": "select",
            "data": {
              "functionCall": "getFallbackValueSelection"
            }
          },
          "filterProps": {
            "type": "group",
            "data": {
              "type": "list",
              "label": {
                "type": "text"
              },
              "value": {
                "type": "select",
                "data": {
                  "functionCall": "getOptionsForFieldKey"
                }
              }
            }
          }
        }
      },
      "RangeSlider": {
        "useWith": {
          "type": "number"
        },
        "defaultParameters": {
          "type": "RangeSlider",
          "comparisonOperator": "range",
          "filterProps": {
            "min": 0,
            "max": 100,
            "step": 1,
            "labelAlwaysOn": false
          }
        },
        "options": {
          "fallback": {
            "hide": true
          },
          "filterProps": {
            "type": "group",
            "label": {
              "type": "select",
              "label": "Label",
              "data": [
                {
                  "label": "Money ($)",
                  "value": "(value) => { return `\\$${value.toLocaleString('en')}`;}"
                },
                {
                  "label": "Squared Feet",
                  "value": "(value) => { return `${value} piesÂ²`;}"
                },
                {
                  "label": "Acres",
                  "value": "(value) => { return `${value} ac.`;}"
                },
                {
                  "label": "Hectares",
                  "value": "(value) => { return `${value} ha.`;}"
                }
              ]
            },
            "min": {
              "type": "number",
              "label": "Start of range"
            },
            "max": {
              "type": "number",
              "label": "End of range"
            },
            "step": {
              "type": "number",
              "label": "Step by:"
            },
            "labelAlwaysOn": {
              "type": "checkbox",
              "label": "Label Always On"
            }
          }
        }
      },
      "SegmentedControl": {
        "useWith": {
          "include": ["bathrooms", "bedrooms"]
        },
        "defaultParameters": {
          "type": "SegmentedControl",
          "comparisonOperator": "===",
          "fallback": "ANY",
          "filterProps": {
            "data": [
              { "label": "1", "value": "1" },
              { "label": "2", "value": "2" },
              { "label": "3", "value": "3" },
              { "label": "4+", "value": ">=4" },
              { "label": "any", "value": "ANY" }
            ]
          }
        },
        "options": {
          "fallback": {
            "type": "select",
            "data": {
              "functionCall": "getFallbackValueSelection"
            }
          },
          "legend": {
            "hide": true
          },
          "filterProps": {
            "type": "group",
            "data": {
              "type": "list",
              "label": {
                "type": "text"
              },
              "value": {
                "type": "text"
              }
            }
          }
        }
      },
      "CheckboxList": {
        "useWith": {
          "type": "boolean"
        },
        "defaultParameters": {
          "type": "CheckboxList",
          "legend": "Checklist",
          "children": [
            {
              "name": "option1",
              "fieldKey": "{nextBoolean}",
              "type": "Checkbox",
              "fallback": false,
              "filterProps": {
                "label": "Option1"
              }
            }
          ]
        },
        "options": {
          "fieldKey": {
            "hide": true
          },
          "fallback": {
            "hide": true
          },
          "children": {
            "type": "list",
            "filterProps.label": {
              "type": "text"
            },
            "fieldKey": {
              "type": "select",
              "data": {
                "functionCall": "getCompatibleFieldKeys"
              }
            }
          }
        }
      },
      "RadioButtonGroup": {
        "useWith": {
          "type": "array"
        },
        "defaultParameters": {
          "type": "RadioButtonGroup",
          "filterValue": "Option1",
          "fallback": "Option3",
          "filterProps": {
            "data": [
              { "label": "Option1", "value": "Option1" },
              { "label": "Option2", "value": "Option2" },
              { "label": "Option3", "value": "Option3" }
            ]
          }
        },
        "options": {
          "fallback": {
            "type": "select",
            "data": {
              "functionCall": "getFallbackValueSelection"
            }
          },
          "filterProps": {
            "type": "group",
            "data": {
              "type": "list",
              "label": {
                "type": "text"
              },
              "value": {
                "type": "select",
                "data": {
                  "functionCall": "getOptionsForFieldKey"
                }
              }
            }
          }
        }
      }
    }
  }
}
